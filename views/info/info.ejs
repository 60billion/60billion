<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <%- include('../parts/bootstrap.ejs'); %>
    <%- include('../parts/css.ejs'); %>
    <%- include('../parts/js.ejs'); %>
  </head>
  <body>
    <div class="container-fluid">

      <!-- first row -->
      <div class="row"><%- include('../parts/top1.ejs'); %></div>

      <!-- Third Row(houseList) -->
      <div class="row" style="margin-top:70px;">
        <div class="col-md-8 col-md-offset-2">
          <div class="row" style=" margin-left:25px;">
            <form class="form-inline">
                <div class="form-group">
                  <select class="selectpicker" multiple data-selected-text-format="count > 2">
                    <option>매매</option>
                    <option>랜트</option>
                    <option>서블랫</option>
                    <option>룸매이트</option>
                  </select>
                  <select class="selectpicker" multiple data-selected-text-format="count > 2">
                    <option>스튜디오</option>
                    <option>1 배드룸</option>
                    <option>2 배드룸</option>
                    <option>3 배드룸</option>
                  </select>
                  <select class="selectpicker" multiple data-selected-text-format="count > 2">
                    <option>$0~</option>
                    <option>$1000~</option>
                    <option>$2000~</option>
                    <option>$3000~</option>
                  </select>
                  <button class="btn btn-default" type="submit">적용</button>
                </div>
              </form>

          </div>
          <div class="row" style=" margin-left:10px;">
            <hr/>
            <!-- target data 일치되어서 3종으로 나눔 추후엔 하나 로직만 이해하면됨 -->
            <%- include('../job/jobSampleList.ejs'); %>
            <%- include('../job/jobSampleList1.ejs'); %>
            <%- include('../job/jobSampleList2.ejs'); %>

          </div>

        </div>
      </div>
      <div class="row" id="bottom"></div>

      <!-- Fourth Row (jobList) -->


  </body>
</html>
